{
  "name": "switchline-backend",
  "version": "1.0.0",
  "description": "Switchline anonymous phone number service backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:backup": "./scripts/backup.sh",
    "deploy": "pm2 start ecosystem.config.js --env production",
    "logs": "pm2 logs",
    "monitor": "pm2 monit"
  },
  "keywords": [
    "burner-phone",
    "anonymous",
    "privacy",
    "twilio",
    "voip",
    "sms"
  ],
  "author": "Switchline Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "twilio": "^4.14.0",
    "pg": "^8.11.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.1",
    "nodemailer": "^6.9.3",
    "stripe": "^12.9.0",
    "bull": "^4.10.4",
    "ioredis": "^5.3.2",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "crypto": "^1.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.43.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-security": "^1.7.1",
    "@types/node": "^20.3.1",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/switchline-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/switchline-backend/issues"
  },
  "homepage": "https://switchline.app"
}